/* pages/rankList/rankList.wxss */
.rankListTitle{
  width: 750rpx;
  height:100rpx;
  font-size: 50rpx;
  justify-content: center;
  text-align: center;
  align-items: center;
} 
.yourRank{
  font-size: 36rpx;
  width: 750rpx;
  text-align: center;

}
.avatar-parent{
  width:60px;
  height:60px;
  border-radius: 50%;
  overflow:hidden;
}
.avatar-image{
  width:60px;
  height:60px;
}
.nickname{
  width: 300rpx;
  display:inline-flex;
  margin-left:10rpx;
  font-size: 36rpx;
  color:black;
  overflow: hidden;
}
.grade{
  margin-left:10rpx;
  font-size: 20rpx;
  color:gray;
}
.rankNumber{
  position:absolute;
  right:20rpx;
  font-size: 36rpx;
  color:black;
}
